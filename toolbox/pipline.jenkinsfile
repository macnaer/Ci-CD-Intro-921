#!groovy
//  groovy Jenkinsfile
properties([disableConcurrentBuilds()])

pipeline  {
    agent { 
        label 'master'
        }
    options {
        buildDiscarder(logRotator(numToKeepStr: '10', artifactNumToKeepStr: '10'))
        timestamps()
    }
    stages {
        stage("Test pipline") {
            steps {
                echo 'Tesing pipline ...'
                dir('.'){
                    sh " echo 'Hello pipline' "
                }
            }
        }
    }
}